    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: CTS",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/application.py",
            "env": {
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0",
                "GOPATH": "/Users/wendell/go",
                "RESOURCE_PATH": "${workspaceFolder}/parser_io/"
            },
            "gevent": true,
            "jinja": true
        },
        {
            "name": "Python: CTS DEBUG",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/application.py",
            "env": {
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0",
                "RESOURCE_PATH": "${workspaceFolder}/parser_io/"
            },
            "args": [
                "--log",
                "DEBUG"
            ],
            "gevent": true,
            "jinja": true
        }
    ]
}